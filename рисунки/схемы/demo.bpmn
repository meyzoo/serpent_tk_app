<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
  id="Definitions_GMSIT_ASIS"
  targetNamespace="http://gms-it.example/practice/asis">

  <bpmn:collaboration id="Collaboration_GMSIT_ASIS">
    <bpmn:participant id="Participant_GMSIT_ASIS"
      name="ООО «ГМС ИНФОРМАЦИОННЫЕ ТЕХНОЛОГИИ» — Service Desk (AS-IS)"
      processRef="Process_GMSIT_ASIS"/>
  </bpmn:collaboration>

  <bpmn:process id="Process_GMSIT_ASIS" isExecutable="false" name="Service Desk AS-IS">
    <bpmn:laneSet id="LaneSet_GMSIT_ASIS">
      <bpmn:lane id="Lane_Requester_ASIS" name="Заявитель (сотрудник/пользователь)">
        <bpmn:flowNodeRef>Start_ASIS</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Submit_ASIS</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Receive_ASIS</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>End_ASIS</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_SD_ASIS" name="Service Desk / инженер поддержки">
        <bpmn:flowNodeRef>Task_Accept_ASIS</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_RequestData_ASIS</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Resolve_ASIS</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Store_ASIS</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_Send_ASIS</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>

    <bpmn:startEvent id="Start_ASIS" name="Возник инцидент/потребность в поддержке">
      <bpmn:outgoing>Flow_ASIS_1</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:task id="Task_Submit_ASIS" name="Создать заявку (портал/почта/телефон)">
      <bpmn:incoming>Flow_ASIS_1</bpmn:incoming>
      <bpmn:outgoing>Flow_ASIS_2</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_Accept_ASIS" name="Принять и зарегистрировать тикет">
      <bpmn:incoming>Flow_ASIS_2</bpmn:incoming>
      <bpmn:outgoing>Flow_ASIS_3</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_RequestData_ASIS" name="Запросить/получить данные (логи/скриншоты/конфиги)">
      <bpmn:incoming>Flow_ASIS_3</bpmn:incoming>
      <bpmn:outgoing>Flow_ASIS_4</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_Resolve_ASIS" name="Диагностика и устранение (удалённо/на месте)">
      <bpmn:incoming>Flow_ASIS_4</bpmn:incoming>
      <bpmn:outgoing>Flow_ASIS_5</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_Store_ASIS" name="Сохранить материалы в общей папке/переписке (без шифрования)">
      <bpmn:incoming>Flow_ASIS_5</bpmn:incoming>
      <bpmn:outgoing>Flow_ASIS_6</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_Send_ASIS" name="Отправить решение/ответ (почта/портал)">
      <bpmn:incoming>Flow_ASIS_6</bpmn:incoming>
      <bpmn:outgoing>Flow_ASIS_7</bpmn:outgoing>
    </bpmn:task>

    <bpmn:task id="Task_Receive_ASIS" name="Получить ответ и подтвердить закрытие">
      <bpmn:incoming>Flow_ASIS_7</bpmn:incoming>
      <bpmn:outgoing>Flow_ASIS_8</bpmn:outgoing>
    </bpmn:task>

    <bpmn:endEvent id="End_ASIS" name="Тикет закрыт">
      <bpmn:incoming>Flow_ASIS_8</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_ASIS_1" sourceRef="Start_ASIS" targetRef="Task_Submit_ASIS"/>
    <bpmn:sequenceFlow id="Flow_ASIS_2" sourceRef="Task_Submit_ASIS" targetRef="Task_Accept_ASIS"/>
    <bpmn:sequenceFlow id="Flow_ASIS_3" sourceRef="Task_Accept_ASIS" targetRef="Task_RequestData_ASIS"/>
    <bpmn:sequenceFlow id="Flow_ASIS_4" sourceRef="Task_RequestData_ASIS" targetRef="Task_Resolve_ASIS"/>
    <bpmn:sequenceFlow id="Flow_ASIS_5" sourceRef="Task_Resolve_ASIS" targetRef="Task_Store_ASIS"/>
    <bpmn:sequenceFlow id="Flow_ASIS_6" sourceRef="Task_Store_ASIS" targetRef="Task_Send_ASIS"/>
    <bpmn:sequenceFlow id="Flow_ASIS_7" sourceRef="Task_Send_ASIS" targetRef="Task_Receive_ASIS"/>
    <bpmn:sequenceFlow id="Flow_ASIS_8" sourceRef="Task_Receive_ASIS" targetRef="End_ASIS"/>
  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_GMSIT_ASIS">
    <bpmndi:BPMNPlane id="BPMNPlane_GMSIT_ASIS" bpmnElement="Collaboration_GMSIT_ASIS">

      <bpmndi:BPMNShape id="Participant_GMSIT_ASIS_di" bpmnElement="Participant_GMSIT_ASIS" isHorizontal="true">
        <dc:Bounds x="60" y="60" width="2000" height="460"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_Requester_ASIS_di" bpmnElement="Lane_Requester_ASIS" isHorizontal="true">
        <dc:Bounds x="90" y="60" width="1970" height="220"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Lane_SD_ASIS_di" bpmnElement="Lane_SD_ASIS" isHorizontal="true">
        <dc:Bounds x="90" y="280" width="1970" height="240"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Start_ASIS_di" bpmnElement="Start_ASIS">
        <dc:Bounds x="140" y="140" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_Submit_ASIS_di" bpmnElement="Task_Submit_ASIS">
        <dc:Bounds x="220" y="120" width="190" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_Accept_ASIS_di" bpmnElement="Task_Accept_ASIS">
        <dc:Bounds x="220" y="340" width="190" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_RequestData_ASIS_di" bpmnElement="Task_RequestData_ASIS">
        <dc:Bounds x="460" y="340" width="260" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_Resolve_ASIS_di" bpmnElement="Task_Resolve_ASIS">
        <dc:Bounds x="760" y="340" width="240" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_Store_ASIS_di" bpmnElement="Task_Store_ASIS">
        <dc:Bounds x="1040" y="340" width="300" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_Send_ASIS_di" bpmnElement="Task_Send_ASIS">
        <dc:Bounds x="1380" y="340" width="220" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="Task_Receive_ASIS_di" bpmnElement="Task_Receive_ASIS">
        <dc:Bounds x="1380" y="120" width="220" height="80"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNShape id="End_ASIS_di" bpmnElement="End_ASIS">
        <dc:Bounds x="1720" y="140" width="36" height="36"/>
      </bpmndi:BPMNShape>

      <bpmndi:BPMNEdge id="Flow_ASIS_1_di" bpmnElement="Flow_ASIS_1">
        <di:waypoint x="176" y="158"/>
        <di:waypoint x="220" y="160"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_ASIS_2_di" bpmnElement="Flow_ASIS_2">
        <di:waypoint x="315" y="200"/>
        <di:waypoint x="315" y="340"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_ASIS_3_di" bpmnElement="Flow_ASIS_3">
        <di:waypoint x="410" y="380"/>
        <di:waypoint x="460" y="380"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_ASIS_4_di" bpmnElement="Flow_ASIS_4">
        <di:waypoint x="720" y="380"/>
        <di:waypoint x="760" y="380"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_ASIS_5_di" bpmnElement="Flow_ASIS_5">
        <di:waypoint x="1000" y="380"/>
        <di:waypoint x="1040" y="380"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_ASIS_6_di" bpmnElement="Flow_ASIS_6">
        <di:waypoint x="1340" y="380"/>
        <di:waypoint x="1380" y="380"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_ASIS_7_di" bpmnElement="Flow_ASIS_7">
        <di:waypoint x="1490" y="340"/>
        <di:waypoint x="1490" y="200"/>
      </bpmndi:BPMNEdge>

      <bpmndi:BPMNEdge id="Flow_ASIS_8_di" bpmnElement="Flow_ASIS_8">
        <di:waypoint x="1600" y="160"/>
        <di:waypoint x="1720" y="158"/>
      </bpmndi:BPMNEdge>

    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>

</bpmn:definitions>